<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  jcr:primaryType="cq:Dialog"
  title="Edit Sample Component"
  height="600"
  width="600"
  xtype="dialog">
  <items jcr:primaryType="cq:WidgetCollection">
    <tabPanel
      jcr:primaryType="cq:Widget"
      xtype="tabpanel">
      <items jcr:primaryType="cq:WidgetCollection">
        <general
          jcr:primaryType="cq:Panel"
          title="General">
          <items jcr:primaryType="cq:WidgetCollection">
            <text
              jcr:primaryType="cq:Widget"
              fieldLabel="Text"
              name="./text"
              xtype="textfield" />
            <nestedText
              jcr:primaryType="cq:Widget"
              fieldLabel="Nexted text"
              name="./nested/text"
              xtype="textfield" />
          </items>
        </general>
      </items>
    </tabPanel>
  </items>
</jcr:root>
