<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  jcr:primaryType="cq:Page">
  <jcr:content
    jcr:primaryType="cq:PageContent"
    jcr:title="Slung work page"
    sling:resourceType="slung/renderers/renderer">
    <paragraphSystem
      jcr:primaryType="nt:unstructured"
      sling:resourceType="foundation/components/parsys">
      <component1
        jcr:primaryType="nt:unstructured"
        text="jsp text"
        richText="&lt;p&gt;jsp rich text&lt;/p&gt;"
        condition="{Boolean}true"
        sling:resourceType="slung/components/slice/jsp/component">
        <nested
          jcr:primaryType="nt:unstructured"
          text="nested text"/>
        <links
          jcr:primaryType="nt:unstructured">
          <link0
            jcr:primaryType="nt:unstructured"
            title="link 0"
            url="some url 0"/>
          <link1
            jcr:primaryType="nt:unstructured"
            title="link 1"
            url="some url 1"/>
          <link2
            jcr:primaryType="nt:unstructured"
            title="link 2"
            url="some url 2"/>
        </links>
      </component1>
      <component2
        jcr:primaryType="nt:unstructured"
        text="jsp text"
        richText="&lt;p&gt;jsp rich text&lt;/p&gt;"
        condition="{Boolean}true"
        sling:resourceType="slung/components/sling/jsp/component">
        <nested
          jcr:primaryType="nt:unstructured"
          text="nested text"/>
        <links
          jcr:primaryType="nt:unstructured">
          <link0
            jcr:primaryType="nt:unstructured"
            title="link 0"
            url="some url 0"/>
          <link1
            jcr:primaryType="nt:unstructured"
            title="link 1"
            url="some url 1"/>
          <link2
            jcr:primaryType="nt:unstructured"
            title="link 2"
            url="some url 2"/>
        </links>
      </component2>
    </paragraphSystem>
  </jcr:content>
</jcr:root>
